<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.HBox?>
<HBox alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.pb.wi.sbd.controllers.ToolbarController">
    <children>
        <ImageView fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/logo.jpg" />
            </image>
        </ImageView>
        <ImageView fitWidth="600.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/baner.jpg" />
            </image>
        </ImageView>
        <ImageView fx:id="logo_hodowla" cache="true" cacheHint="SPEED" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/logo_1.png" />
            </image>
        </ImageView>
    </children>
</HBox>