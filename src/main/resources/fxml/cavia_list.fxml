<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane fx:id="root_anchor" stylesheets="@../stylesheet.css" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="622.0" prefWidth="901.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pl.edu.pb.wi.sbd.controllers.TableCaviaController">
    <children>
        <Pane layoutX="131.0" layoutY="6.0" prefHeight="173.0">
            <children>
                <fx:include fx:id="banner" source="banner.fxml"/>
            </children>
        </Pane>
        <TableView fx:id="table_cavia" layoutX="-1.0" layoutY="173.0" prefHeight="380.0" prefWidth="901.0">
            <columns>
                <TableColumn fx:id="column_lp" prefWidth="31.0" text="Lp"/>
                <TableColumn fx:id="column_name" prefWidth="119.0" text="Imię"/>
                <TableColumn fx:id="column_hodowla" prefWidth="75.0" text="Przydomek"/>
                <TableColumn fx:id="column_sex" prefWidth="75.0" text="Płeć"/>
                <TableColumn fx:id="column_age" prefWidth="75.0" text="Wiek"/>
                <TableColumn fx:id="column_weight" prefWidth="75.0" text="Waga"/>
                <TableColumn fx:id="column_race" prefWidth="75.0" text="Rasa"/>
                <TableColumn fx:id="column_colour" prefWidth="100.0" text="Umaszczenie"/>
                <TableColumn fx:id="column_mom" prefWidth="50.0" text="Mama"/>
                <TableColumn fx:id="column_dad" prefWidth="75.0" text="Tata"/>
            </columns>
        </TableView>
        <Button fx:id="button_add_cavia" layoutX="806.0" layoutY="583.0" mnemonicParsing="false"
                onAction="#actionAddNewCavia" text="Dodaj kawię"/>
        <Button fx:id="button_back" layoutX="14.0" layoutY="583.0" mnemonicParsing="false" onAction="#actionBack"
                text="Powrót"/>
        <Button fx:id="button_remove_cavia" layoutX="708.0" layoutY="583.0" mnemonicParsing="false"
                onAction="#actionDeleteSelectedCavia" text="Usuń kawię"/>
        <VBox layoutX="833.0" layoutY="14.0" prefHeight="25.0" prefWidth="54.0">
            <children>
                <fx:include fx:id="resizez" source="buttonResizer.fxml"/>
            </children>
        </VBox>
    </children>

</AnchorPane>
